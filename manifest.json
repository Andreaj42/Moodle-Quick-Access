{
    "manifest_version": 3,
    "name": "Moodle Quick Access",
    "version": "1.0",
    "description": "Log injector for Moodle",
    "permissions": ["storage", "scripting", "activeTab"],
    "action": {
      "default_popup": "/src/popup/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/content/logMoodle.js"]
      }
    ]
}
  